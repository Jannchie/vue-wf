{
  "name": "vue-wf-workspace",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm -C packages/vue-wf dev",
    "dev:all": "pnpm -r --parallel --filter ./packages/** dev",
    "build": "pnpm -r --filter ./packages/** build",
    "watch": "pnpm -C packages/vue-wf watch",
    "preview": "pnpm -C packages/vue-wf preview",
    "docs:dev": "pnpm -C packages/docs dev",
    "docs:build": "pnpm -C packages/docs build",
    "docs:preview": "pnpm -C packages/docs preview",
    "lint": "eslint packages",
    "lint:fix": "eslint packages --fix"
  },
  "devDependencies": {
    "@jannchie/eslint-config": "^3.7.1",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3"
  },
  "pnpm": {
    "overrides": {
      "vue": "^3.0.0"
    }
  },
  "resolutions": {
    "vue": "^3.0.0"
  }
}